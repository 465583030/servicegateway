{
  "applications": {
    "identity": {
      "routing": {
        "path": "/identity",
        "type": "path"
      },
      "auth": {
        "disable": true
      },
      "backend": {
        "url": "http://identity.service.consul"
      },
      "rate_limiting": true
    },
    "flow": {
      "caching": {
        "auto_flush": true,
        "enabled": true,
        "ttl": 3600
      },
      "routing": {
        "path": "/foobar",
        "type": "path"
      },
      "backend": {
        "url": "http://localhost"
      }
    },
    "httpbin": {
      "caching": {
        "auto_flush": true,
        "enabled": true,
        "ttl": 3600
      },
      "routing": {
        "path": "/bin",
        "type": "path"
      },
      "backend": {
        "url": "http://httpbin.org"
      },
      "rate_limiting": true
    },
    "cart_promotion": {
      "routing": {
        "patterns": {
          "/cart/:id/promotion": "/promotion/:id"
        },
        "type": "pattern"
      },
      "backend": {
        "url": "http://cart-promotion.service.consul"
      },
      "rate_limiting": true
    },
    "cart": {
      "routing": {
        "path": "/cart",
        "type": "path"
      },
      "backend": {
        "url": "http://cart.service.consul/rest/v2"
      },
      "rate_limiting": true
    }
  },
  "rate_limiting": {
    "window": "15m",
    "burst": 1000
  },
  "consul": {
    "url": "http://localhost:8500"
  },
  "authentication": {
    "mode": "graphical",
    "storage": {
      "mode": "session",
      "name": "COAB_SESSION",
      "cookie_domain": "localhost:8080",
      "cookie_secure": false,
      "cookie_httponly": true
    },
    "graphical": {
      "login_route": "/login"
    },
    "provider": {
      "url": "http://identity.services.mittwald.it",
      "parameter_format": "json",
      "parameters": {
        "token_ttl": 86400,
        "providers": [
          "public"
        ]
      }
    },
    "verification_key": null,
    "verification_key_url": "http://identity.services.mittwald.it/key",
    "key_cache_ttl": "5m"
  }
}