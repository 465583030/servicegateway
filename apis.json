{
  "applications": {
    "identity": {
      "routing": {
        "path": "/identity",
        "type": "path"
      },
      "auth": {
        "disable": true
      },
      "backend": {
        "url": "http://identity.service.consul"
      },
      "rate_limiting": true
    },
    "flow": {
      "caching": {
        "auto_flush": true,
        "enabled": true,
        "ttl": 3600
      },
      "routing": {
        "path": "/foobar",
        "type": "path"
      },
      "backend": {
        "url": "http://localhost"
      }
    },
    "httpbin": {
      "caching": {
        "auto_flush": true,
        "enabled": true,
        "ttl": 3600
      },
      "routing": {
        "path": "/bin",
        "type": "path"
      },
      "backend": {
        "url": "http://httpbin.org"
      },
      "rate_limiting": true
    },
    "cart_promotion": {
      "routing": {
        "patterns": {
          "/cart/:id/promotion": "/promotion/:id"
        },
        "type": "pattern"
      },
      "backend": {
        "url": "http://cart-promotion.service.consul"
      },
      "rate_limiting": true
    },
    "cart": {
      "routing": {
        "path": "/cart",
        "type": "path"
      },
      "backend": {
        "url": "http://cart.service.consul/rest/v2"
      },
      "rate_limiting": true
    }
  },
  "rate_limiting": {
    "window": "15m",
    "burst": 1000
  },
  "authentication": {
    "mode": "graphical",
    "storage": {
      "mode": "cookie",
      "name": "coab"
    },
    "graphical": {
      "login_route": "/login"
    },
    "verification_key": "asd",
    "verification_key_url": null,
    "key_cache_ttl": "5m"
  }
}